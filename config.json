{
    "warnOnUnexpectedLayer": "true",
    "wv-options-fetch": [
        {
            "from": "https://map1.vis.earthdata.nasa.gov/wmts-geo/wmts.cgi?request=GetCapabilities",
            "to": "gibs-geographic.xml"
        },
        {
            "from": "https://map1.vis.earthdata.nasa.gov/wmts-arctic/wmts.cgi?request=GetCapabilities",
            "to": "gibs-arctic.xml"
        },
        {
            "from": "https://map1.vis.earthdata.nasa.gov/wmts-antarctic/wmts.cgi?request=GetCapabilities",
            "to": "gibs-antarctic.xml"
        },
        {
            "from": "https://map1.vis.earthdata.nasa.gov/wmts-webmerc/wmts.cgi?request=GetCapabilities",
            "to": "gibs-webmerc.xml"
        }
    ],
    "wv-options-wmts": [
        {
            "source": "GIBS:geographic",
            "from": "gibs-geographic.xml",
            "to": "gibs-geographic.json",
            "projection": "geographic",
            "maxResolution": 0.5625
        },
        {
            "source": "GIBS:arctic",
            "from": "gibs-arctic.xml",
            "to": "gibs-arctic.json",
            "projection": "arctic",
            "maxResolution": 8192.0
        },
        {
            "source": "GIBS:antarctic",
            "from": "gibs-antarctic.xml",
            "to": "gibs-antarctic.json",
            "projection": "antarctic",
            "maxResolution": 8192.0
        },
        {
            "source": "GIBS:webmerc",
            "from": "gibs-webmerc.xml",
            "to": "gibs-webmerc.json",
            "projection": "webmerc",
            "maxResolution": 156543.03390625
        }
    ]
}
