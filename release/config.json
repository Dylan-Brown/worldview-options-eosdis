{
    "warnOnUnexpectedLayer": "true",
    "wv-options-fetch": [
        {
            "from": "http://gibs.earthdata.nasa.gov/wmts/epsg4326/best/wmts.cgi?request=GetCapabilities",
            "to": "gibs-geographic.xml"
        },
        {
            "from": "http://gibs.earthdata.nasa.gov/wmts/epsg3413/best/wmts.cgi?request=GetCapabilities",
            "to": "gibs-arctic.xml"
        },
        {
            "from": "http://gibs.earthdata.nasa.gov/wmts/epsg3031/best/wmts.cgi?request=GetCapabilities",
            "to": "gibs-antarctic.xml"
        },
        {
            "from": "http://gibs.earthdata.nasa.gov/wmts/epsg3857/best/wmts.cgi?request=GetCapabilities",
            "to": "gibs-epsg3857-best.xml"
        }
    ],
    "wv-options-wmts": [
        {
            "source": "GIBS:geographic",
            "from": "gibs-geographic.xml",
            "to": "gibs-geographic.json",
            "projection": "geographic",
            "maxResolution": 0.5625
        },
        {
            "source": "GIBS:arctic",
            "from": "gibs-arctic.xml",
            "to": "gibs-arctic.json",
            "projection": "arctic",
            "maxResolution": 8192.0
        },
        {
            "source": "GIBS:antarctic",
            "from": "gibs-antarctic.xml",
            "to": "gibs-antarctic.json",
            "projection": "antarctic",
            "maxResolution": 8192.0
        },
        {
            "source": "GIBS:epsg3857:best",
            "from": "gibs-epsg3857-best.xml",
            "to": "gibs-epsg3857-best.json",
            "projection": "webmerc",
            "maxResolution": 156543.03390625
        }
    ]
}
